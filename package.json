{
    "name": "groundhog-basement",
    "version": "0.0.1",
    "description": "start of the journey",
    "type": "module",
    "source": "src/index.html",
    "scripts": {
        "start": "parcel ./src/index.html --no-cache --config ./parcel/.parcel.dev --open",
        "build": "parcel build src/index.html --dist-dir dist --public-url _relativeroute_ --no-source-maps --log-level error --config ./parcel/.parcel.prod",
        "build2": "parcel build src/index.html --dist-dir dist"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "dijkstra": "https://github.com/warlockxins/dijkstra.git",
        "express": "^4.19.2",
        "phaser": "^3.80.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.29.0",
        "@typescript-eslint/parser": "^2.29.0",
        "eslint": "^6.8.0",
        "parcel": "^2.12.0",
        "parcel-reporter-clean-dist": "^1.0.4",
        "parcel-reporter-static-files-copy": "^1.5.3",
        "process": "^0.11.10"
    },
    "parcelCleanPaths": [
        "dist"
    ],
    "staticFiles": {
        "staticPath": "public",
        "watcherGlob": "**"
    }
}
